package selfProgramming;

public class Boolean2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        boolean A = true;
        boolean B = false;
        
        System.out.println("A|B = " + (A|B));
        System.out.println("A&B = " + (A&B));
        System.out.println("!A = " + (!A));
        System.out.println("A^B = " + (A^B));
        System.out.println("(A|B)&A = " + ((A|B)&A));
	}
}
